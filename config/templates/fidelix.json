{
  "plugins": [
    "soap-ntlm"
  ],
  "protocol": "soap",
  "authConfig": {
    "url": "http://${ipAddress}/WebServices/Fidelix.asmx?WSDL",
    "username": "${username}",
    "password": "${password}",
    "soapPath": "Fidelix.FidelixSoap.getPointData",
    "resourcePath": {
      "point_id": "${id}"
    }
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "${type}": "getPointDataResult"
  },
  "generalConfig": {
    "hardwareId": {
      "dataObjectProperty": "hardwareId"
    }
  }
}
