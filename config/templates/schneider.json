{
  "plugins": [
    "oauth2",
    "schneider"
  ],
  "protocol": "rest",
  "authConfig": {
    "url": "${url}",
    "authPath": "/GetToken",
    "username": "${username}",
    "password": "${password}",
    "resourcePath": "/Values/${id}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "${type}": "Value"
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "Name"
    },
    "hardwareId": {
      "dataObjectProperty": "Id"
    }
  }
}
