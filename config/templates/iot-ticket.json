{
  "plugins": [
    "iot-ticket",
    "basic"
  ],
  "protocol": "rest",
  "authConfig": {
    "resourcePath": "/process/read/${deviceId}?datanodes=${path}",
    "url": "https://my.iot-ticket.com/api/v1",
    "username": "${username}",
    "password": "${password}"
  },
  "dataObjects": [
    "datanodeReads"
  ],
  "dataPropertyMappings": {
    "${type}": "values.0.v"
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "name"
    },
    "hardwareId": {
      "dataObjectProperty": "path"
    },
    "timestamp": {
      "dataObjectProperty": "values.0.ts"
    }
  }
}
